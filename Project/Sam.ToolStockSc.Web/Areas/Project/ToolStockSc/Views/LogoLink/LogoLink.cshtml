@model Sam.ToolStockSc.Web.Areas.Project.ToolStockSc.Models.ViewModels.LogoLinkViewModel
@if (Model.Link != null)
{
    <a class="navbar-brand" href="@Model.Link.Url">
        <img src=@Model.Logo.Src alt="Logo">
    </a>
}
else
{
    <a class="navbar-brand" href="">
        <img src="" alt="Logo">
    </a>
}
