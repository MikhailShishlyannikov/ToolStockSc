@{
    Layout = null;
}

<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js h-100" lang="">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <meta name="description" content="Ela Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="shortcut icon" href="~/Content/Images/favicon.ico" type="image/x-icon">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/pixeden-stroke-7-icon@1.2.3/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css">
    <link rel="stylesheet" href="~/Content/normalize.css" />
    <link rel="stylesheet" href="~/Content/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/cs-skin-elastic.css" />
    <link rel="stylesheet" href="~/Content/dataTables.bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/style.css" />
    <link rel="stylesheet" href="~/Content/chartist.css" />
    <link rel="stylesheet" href="~/Content/jqvmap.css" />
    <link rel="stylesheet" href="~/Content/weather-icons.css" />
    <link rel="stylesheet" href="~/Content/fullcalendar.css" />
    <title>@Html.Sitecore().Field("Title")</title>

    <!-- <script type="text/javascript" src="https://cdn.jsdelivr.net/html5shiv/3.7.3/html5shiv.min.js"></script> -->
    <style>
        #weatherWidget .currentDesc {
            color: #ffffff !important;
        }

        .traffic-chart {
            min-height: 335px;
        }

        #flotPie1 {
            height: 150px;
        }

            #flotPie1 td {
                padding: 3px;
            }

            #flotPie1 table {
                top: 20px !important;
                right: -10px !important;
            }

        .chart-container {
            display: table;
            min-width: 270px;
            text-align: left;
            padding-top: 10px;
            padding-bottom: 10px;
        }

        #flotLine5 {
            height: 105px;
        }

        #flotBarChart {
            height: 150px;
        }

        #cellPaiChart {
            height: 160px;
        }

        .scEmptyPlaceholder {
            border: 1px dotted red;
        }

        .left-panel .scEmptyPlaceholder {
            width: 200px;
        }

        #header .scEmptyPlaceholder {
            width: 200px;
        }

        #main-content .row .scEmptyPlaceholder {
            width: 100%;
        }

        .ml-auto .scEmptyPlaceholder {
            width: 400px !important;
            margin-right: 155px;
        }

        .ml-auto .scTextWrapper {
            margin-right: 155px;
        }

        .ml-auto .scEnabledChrome {
            margin-right: 155px;
        }
    </style>
</head>

<body class="h-100">
    @{HtmlHelper.ClientValidationEnabled = true;}
    @{HtmlHelper.UnobtrusiveJavaScriptEnabled = true;}
    <!-- Left Panel -->
    @if (Sitecore.Context.PageMode.IsExperienceEditorEditing)
    {
        <aside id="left-panel" class="left-panel">
            <div style="padding-left: 5px; margin-top: 100px;">
                @Html.Sitecore().Placeholder("Left menu")
            </div>
        </aside>
    }
    else
    {
        <aside id="left-panel" class="left-panel">
            @Html.Sitecore().Placeholder("Left menu")
        </aside>
    }

    <!-- /#left-panel -->
    <!-- Right Panel -->
<div id="right-panel" class="right-panel h-100">
    <!-- Header-->
    @Html.Sitecore().Placeholder("HeaderSc")
    <!-- /#header -->
    <!-- Content -->
    <div class="content">
        <!-- Animated -->
        <div class="animated fadeIn">
            <div class="row">
                <div id="main-content" class="col-sm-12">
                    <div class="row">
                        @Html.Sitecore().DynamicPlaceholder("ContentSC")
                    </div>
                </div>
            </div>
        </div>
        <!-- .animated -->
    </div>
    <!-- /.content -->
    <div class="clearfix"></div>
    <!-- Footer -->
    @Html.Sitecore().Placeholder("FooterSc")
    <!-- /.site-footer -->
</div>
    <!-- /#right-panel -->
    <!-- Scripts -->
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/jQuery.tmpl.js"></script>
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    <script src="~/Scripts/bootstrap.bundle.js"></script>
    <script src="~/Scripts/jquery.matchHeight.js"></script>
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/data-table/datatables.min.js"></script>
    <script src="~/Scripts/data-table/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/data-table/buttons.bootstrap.min.js"></script>
    <script src="~/Scripts/data-table/jszip.min.js"></script>
    <script src="~/Scripts/data-table/vfs_fonts.js"></script>
    <script src="~/Scripts/data-table/buttons.html5.min.js"></script>
    <script src="~/Scripts/data-table/buttons.print.min.js"></script>
    <script src="~/Scripts/data-table/buttons.colVis.min.js"></script>
    <script src="~/Scripts/datatables-init.js"></script>
    <script src="~/Scripts/Chart.bundle.js"></script>
    <script src="~/Scripts/chartist.js"></script>
    <script src="~/Scripts/chartist-plugin-legend.js"></script>
    <script src="~/Scripts/jquery.flot.js"></script>
    <script src="~/Scripts/jquery.flot.pie.min.js"></script>
    <script src="~/Scripts/jquery.flot.spline.js"></script>
    <script src="~/Scripts/jquery.simpleWeather.js"></script>
    <script src="~/Scripts/weather-init.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <script src="~/Scripts/fullcalendar.js"></script>
    <script src="~/Scripts/fullcalendar-init.js"></script>
    <script src="~/Scripts/CustomScript.js"></script>
    @*@Scripts.Render("~/Content/Template/js")*@
</body>
</html>

